Algoritmo volumen_Paralelepipedo
	// ingreso de datos
	Definir altura, largo, ancho, volumen Como Real;
	Escribir "Ingrese la altura: ";
	Leer altura;
	Escribir "Ingrese el ancho: ";
	Leer ancho;
	Escribir "Ingrese el largo: ";
	Leer largo;
	
	// Procesamientos de datos
	volumen <- altura * ancho * largo;
	
	// salida
	Escribir "El volumen es: ", volumen;
FinAlgoritmo
